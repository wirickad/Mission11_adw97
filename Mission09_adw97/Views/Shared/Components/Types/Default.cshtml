@model IEnumerable<string>

<!--These are the buttons that go inside the left menu to filter by category-->
<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-categoryType="" asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>
    <br />
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-categoryType="@t" asp-route-pageNum="1" class=" btn hoverable @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
    }

</div>